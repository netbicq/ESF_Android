<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="EsafetyTheme" parent="QMUI.Compat">
        <item name="colorPrimary">#5CACEE</item>
        <item name="colorPrimaryDark">#5CACEE</item>
        <item name="colorAccent">#5CACEE</item>
    </style>

    <style name="BaseTextViewStyle" parent="TextView.Compat">
        <item name="android:textSize">14sp</item>
        <item name="android:color">#515151</item>
        <item name="android:ellipsize">end</item>
        <item name="android:lines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">5dp</item>
    </style>
</resources>
